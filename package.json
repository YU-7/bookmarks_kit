    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.1",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "sql.js": "^1.13.0",
    "tailwindcss": "^4.1.18"
